{
  "name": "tinykode",
  "version": "1.0.0",
  "description": "A lightweight code assistant library with CLI support",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "tinykode": "bin/tinykode.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "start": "node --env-file=.env dist/index.js",
    "cli": "node dist/cli/cli.js",
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.15",
    "ai": "^5.0.41",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^24.4.0",
    "eslint": "^9.35.0",
    "globals": "^16.4.0",
    "typescript": "^5.9.2"
  }
}
